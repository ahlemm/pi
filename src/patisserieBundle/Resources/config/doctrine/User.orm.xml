<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="patisserieBundle\Entity\User" table="user">
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateNaissance" type="date" column="date_naissance" nullable="true"/>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="numTel" type="integer" column="num_tel" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="login" type="string" column="login" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
